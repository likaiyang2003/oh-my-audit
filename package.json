{
  "name": "code-security-audit",
  "version": "1.0.0",
  "description": "Professional Java code security audit plugin for OpenCode",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "security",
    "audit",
    "java",
    "vulnerability",
    "opencode",
    "plugin",
    "static-analysis",
    "owasp",
    "penetration-testing"
  ],
  "author": "likaiyang2003",
  "repository": {
    "type": "git",
    "url": "https://github.com/likaiyang2003/oh-my-audit.git"
  },
  "bugs": {
    "url": "https://github.com/likaiyang2003/oh-my-audit/issues"
  },
  "homepage": "https://github.com/likaiyang2003/oh-my-audit#readme",
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target bun --format esm && tsc --emitDeclarationOnly",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "clean": "rm -rf dist",
    "final-check": "bun run ./scripts/final-check.ts",
    "bump-version": "bun run ./scripts/bump-version.ts"
  },
  "dependencies": {
    "@opencode-ai/plugin": "^1.1.19",
    "@opencode-ai/sdk": "^1.1.19",
    "adm-zip": "^0.5.16",
    "js-yaml": "^4.1.1"
  },
  "devDependencies": {
    "bun-types": "latest",
    "typescript": "^5.7.3",
    "@types/adm-zip": "^0.5.7",
    "@types/js-yaml": "^4.0.9"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
